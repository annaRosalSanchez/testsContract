{
  "name": "testscontract",
  "version": "1.0.0",
  "description": "api contract testing with pact and TypeScript",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf pacts",
    "test": "nyc --reporter=text-summary mocha -r ts-node/register -t 30000 src/consumer/*.spec.ts",
    "publish": "node publisher.js",
    "verify": "nyc --reporter=text-summary mocha -r ts-node/register -t 30000 src/provider/*.spec.ts"
  },
  "keywords": [
    "qa",
    "contract",
    "testing",
    "automation",
    "pact"
  ],
  "author": "Anna Rosal",
  "license": "ISC",
  "devDependencies": {
    "@pact-foundation/absolute-version": "0.0.1",
